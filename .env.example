# WH2900 Weather Station - Environment Variables
# Copy this file to .env and fill in your credentials

# Weathercloud
WEATHERCLOUD_ID=your_wid_here
WEATHERCLOUD_KEY=your_key_here
WEATHERCLOUD_DEVICE_ID=your_device_id_here

# Weather Underground
WUNDERGROUND_ID=YOUR_STATION_ID
WUNDERGROUND_KEY=your_api_key_here

# PWSweather
PWSWEATHER_ID=YOUR_STATION_ID
PWSWEATHER_KEY=your_api_key_here

# Windguru
WINDGURU_UID=your_station_uid
WINDGURU_KEY=your_api_password
